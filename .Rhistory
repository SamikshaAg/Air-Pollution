y <- final[,"nitrate"]
comp <- complete.cases(x,y)
z <- comp
len <- length(z[comp==TRUE])
res1 <- x[comp]
res2 <- y[comp]
if(len>threshold)
correlation <- cor(res1, res2)
}
}
